    "dist:portable": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win portable --publish never",
    "dist:squirrel": "npm run build && node scripts\\squirrel-build.js",
    "dist:web": "npm run build && cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win nsis-web --publish never"
  },
