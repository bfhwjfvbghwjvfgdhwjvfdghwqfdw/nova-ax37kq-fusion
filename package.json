    "start:electron": "electron .",
    "build": "vite build",
    "preview": "vite preview",
    "fix-icon": "node scripts\\fix-icon.js",
    "pack:win": "electron-builder --win nsis",
    "dist:win": "cross-env CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --win nsis --publish never",
